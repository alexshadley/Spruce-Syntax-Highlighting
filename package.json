{
    "name": "spruce-syntax-highlighting",
    "displayName": "Spruce Syntax Highlighting",
    "description": "Syntax highlighting for the Spruce Programming Language",
    "version": "0.0.3",
    "publisher": "spruce",
    "repository": {
        "type": "git",
        "url": "https://github.com/alexshadley/Spruce-Syntax-Highlighting"
      },
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "spruce",
            "aliases": ["Spruce", "spruce"],
            "extensions": [".sp"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "spruce",
            "scopeName": "source.sp",
            "path": "./syntaxes/spruce.tmLanguage.json"
        }]
    }
}